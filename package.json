{
  "name": "contentstack-types-generator",
  "version": "1.0.4",
  "description": "This package generate types for your content types.",
  "main": "dist/index.js",
  "bin": {
    "types-generator": "./dist/index.js"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc --module commonjs && rollup -c rollup.config.js && chmod +x dist/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --watch"
  },
  "keywords": [
    "contentstack",
    "types"
  ],
  "author": "Ravi Lamkoti<ravilamkoti@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.10",
    "jest": "^29.7.0",
    "rollup": "^2.79.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "contentstack": "^3.17.1",
    "prettier": "^3.0.3"
  }
}
